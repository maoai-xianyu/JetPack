<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.NotificationsFragment">

    <TextView
        android:id="@+id/text_notifications"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnJumpC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="ConstraintLayout"
        app:backgroundTint="@color/color_theme"
        app:cornerRadius="0dp"
        app:iconGravity="textStart"
        app:iconPadding="4dp"
        app:iconTint="@color/color_theme"
        app:layout_constraintStart_toStartOf="@+id/text_notifications"
        app:layout_constraintTop_toBottomOf="@+id/text_notifications" />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/btnJumpC2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="ConstraintLayout2.0"
        app:backgroundTint="@color/color_theme"
        app:cornerRadius="0dp"
        app:iconGravity="textStart"
        app:iconPadding="4dp"
        app:iconTint="@color/color_theme"
        app:layout_constraintStart_toEndOf="@+id/btnJumpC"
        app:layout_constraintTop_toBottomOf="@+id/text_notifications" />


</androidx.constraintlayout.widget.ConstraintLayout>