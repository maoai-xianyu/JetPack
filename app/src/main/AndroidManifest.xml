<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mao.jetpack">

    <application
        android:name=".JetPackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.livedata.LiveDataActivity" />
        <activity android:name=".ui.livedata.LiveDataJavaSingleActivity" />
        <activity android:name=".ui.viewmodelfile.ViewModelJavaLiveDataActivity" />
        <activity android:name=".ui.viewmodelfile.ViewModelActivity" />
        <activity android:name=".ui.viewmodelfile.ViewModelJavaActivity" />
        <activity android:name=".ui.room.RoomActivity" />
        <activity android:name=".ui.room.RoomSelectActivity" />
        <activity android:name=".ui.constraintLayout.ConstraintLayoutActivity" />
    </application>

</manifest>